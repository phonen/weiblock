<script>
    var my_height = $("body")[0].clientHeight;
    var my_width = $("body")[0].clientWidth;
    var sdf = my_height/my_width;

    if (sdf < 1.6) {$("body").addClass('suo');};
</script>
<script type="text/javascript">
    $(function(){
    	var needRefresh = sessionStorage.getItem("need-refresh");
        if(needRefresh){
            sessionStorage.removeItem("need-refresh");
            location.reload();
        }
    	
         $('.phbb').click(function(event) {
            $('.phb_box').show();
       });
         $('.qfd').click(function(event) {
            $('.phb_box').hide();
       });
        $('.lop').click(function(event) {
            $('.layer_bg').show();
            $('.slideBox').css('opacity','1')
            $('.slideBox').css('top','2rem')
        });
        $('.layer_bg').click(function(event) {
                 $('.slideBox').css('opacity','0');
                 $('.slideBox').css('top','-50rem');
                 $(".layer_bg,.layer,#layer_redpacket,#ad_layer").fadeOut('400');
        });

         $('.layer_bg').on('touchmove', function(event) {
             event.preventDefault();
         });

         // 红包
         $("#freshgift").on('click', function(event) {
			 $.ajax({
				url:"{:U('Users/signIn')}",
				type:'post',
				dataType:'json',
				success:function(res) {
					if(res['status']==1){
						$('#redpacket_cont').text(res.msg);
						$(".layer_bg,#layer_redpacket").fadeIn('400');
						setTimeout( "location.reload()",2000 );
					} else if(res['status']==-1) {

						$('.layer p').text(res['msg']);
						$('.layer,.layer_bg').show(); 
					}
				}
			});
         });
         
         $(".phbb2").on('click', function(event) {
        	 $.ajax({
                 url:"{:U('Users/first')}",
                 type:'post',
                 dataType:'json',
                 success:function(res) {
                     if(res['status']==1){ 
                     	$('.layer p').text(res['msg']);
                         $('.layer,.layer_bg').show(); 
                         setTimeout( "location.reload()",2000 );
                         // location.reload();
                     } else if(res['status']==-1) {
                         $('.layer p').text(res['msg']);
                         $('.layer,.layer_bg').show(); 
                     }
                     
                 }
             });
         });
    })
    function first(){
    	$.ajax({
            url:"{:U('Users/first')}",
            type:'post',
            dataType:'json',
            success:function(res) {
                if(res['status']==1){ 
                	$('.layer p').text(res['msg']);
                    $('.layer,.layer_bg').show(); 
                    setTimeout( "location.reload()",2000 );
                    // location.reload();
                } else if(res['status']==-1) {
                    $('.layer p').text(res['msg']);
                    $('.layer,.layer_bg').show(); 
                }
                
            }
        });
    }
</script>